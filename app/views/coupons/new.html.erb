<div>
  
  <h2>Coupon Form</h2>
</div>
<div>
  <%= form_tag coupons_path do %>
    <p><%= label_tag :'coupon[coupon_code]', "Coupon Code: " %>
    <%= text_field_tag(
        :'coupon[coupon_code]',nil, id: 'coupon_code'
        )
    %></p>
    <p><%= label_tag :'coupon[store]', "Store: " %>
    <%= text_field_tag(
        :'coupon[store]',nil, id: 'store'
        )
    %></p>
    <p><%= submit_tag "Submit Coupon" %></p>
  <% end %>

</div>
